{
  "name": "mern_auth",
  "version": "1.0.0",
  "description": "Mern Demo with authentication using passport local strategy",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && NPM_CONFIG_PRODUCTION=false npm install && npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "json-server": "^0.16.1",
    "mongoose": "^5.9.14",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reactstrap": "^8.4.1"
  },
  "engines": {
    "node": "12.16.1"
},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanik1320/Evventi.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/shanik1320/Evventi/issues"
  },
  "homepage": "https://github.com/shanik1320/Evventi#readme"
}
